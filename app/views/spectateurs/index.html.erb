<p id="notice"><%= notice %></p>

<h1>Listing Spectateurs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Descripton</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spectateurs.each do |spectateur| %>
      <tr>
        <td><%= spectateur.name %></td>
        <td><%= spectateur.descripton %></td>
        <td><%= link_to 'Show', spectateur %></td>
        <td><%= link_to 'Edit', edit_spectateur_path(spectateur) %></td>
        <td><%= link_to 'Destroy', spectateur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spectateur', new_spectateur_path %>
